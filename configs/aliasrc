# Aliases
alias ls="ls -Fh --group-directories-first"
alias cp="cp -r"
alias rm="rm -r"
alias less="less -x4"
alias feh="feh -Z -B '#000000'"
alias ps="ps -H"
alias dbx="distrobox"
alias l="ls -hla --group-directories-first --file-type"
alias dnf="dnf -C"
